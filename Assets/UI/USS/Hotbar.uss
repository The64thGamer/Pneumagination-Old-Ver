.custom-editbutton {
    flex-grow: 1;
    min-height: 38px;
    min-width: 38px;
    max-width: 38px;
    max-height: 38px;
    border-top-width: 3px;
    border-right-width: 3px;
    border-bottom-width: 3px;
    border-left-width: 3px;
    border-left-color: rgb(30, 33, 30);
    border-right-color: rgb(30, 33, 30);
    border-top-color: rgb(30, 33, 30);
    border-bottom-color: rgb(30, 33, 30);
    background-image: none;
    -unity-background-image-tint-color: rgb(27, 27, 27);
    padding-top: 2px;
    padding-right: 2px;
    padding-bottom: 2px;
    padding-left: 2px;
    margin-right: 2px;
    background-color: rgba(0, 0, 0, 0);
