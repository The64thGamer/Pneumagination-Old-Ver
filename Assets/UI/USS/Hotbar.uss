.unity-progress-bar__container {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    min-height: 38px;
    max-height: 38px;
    flex-grow: 1;
    justify-content: center;
    align-self: center;
    background-color: rgba(255, 0, 0, 0);
    flex-shrink: 0;
}

.unity-progress-bar__background {
    margin-top: 0;
    margin-right: 0;
    margin-bottom: 0;
    margin-left: 0;
    min-height: 38px;
    max-height: 38px;
    flex-grow: 1;
    justify-content: center;
    align-self: center;
    background-color: rgb(30, 33, 30);
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
    flex-shrink: 0;
    flex-direction: row;
}

.unity-progress-bar__progress {
    background-color: rgb(236, 235, 222);
    margin-top: 3px;
    margin-right: 3px;
    margin-bottom: 3px;
    margin-left: 3px;
}

.unity-base-text-field {
