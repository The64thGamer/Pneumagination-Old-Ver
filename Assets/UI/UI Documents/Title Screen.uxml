                    <ui:Button text="Settings" display-tooltip-when-elided="true" name="Settings" class="titlescreenbuttons" style="display: flex;" />
                    <ui:Button text="Exit" display-tooltip-when-elided="true" name="Exit" class="titlescreenbuttons" />
                </ui:VisualElement>
                <ui:VisualElement style="flex-grow: 1;" />
                <ui:Label tabindex="-1" text="Alpha 0.4, The 64th Gamer 2023" display-tooltip-when-elided="true" name="Label" style="font-size: 40px; -unity-font-definition: url(&apos;project://database/Assets/UI/Fonts/PTMono-Regular.ttf?fileID=12800000&amp;guid=cc1735a3c585aba4480bc03c8d93b029&amp;type=3#PTMono-Regular&apos;); color: rgb(255, 255, 255);" />
            </ui:VisualElement>
            <ui:VisualElement name="MenuGenerator" style="flex-grow: 1; margin-top: 60px; margin-right: 60px; margin-bottom: 60px; margin-left: 60px; display: none; align-items: stretch;">
                <ui:Label tabindex="-1" text="Name Generator" display-tooltip-when-elided="true" style="font-size: 40px; -unity-font-style: bold; -unity-font-definition: url(&apos;project://database/Assets/UI/Fonts/PTMono-Regular.ttf?fileID=12800000&amp;guid=cc1735a3c585aba4480bc03c8d93b029&amp;type=3#PTMono-Regular&apos;); color: rgb(255, 255, 255);" />
                <ui:Label tabindex="-1" text="Ok so this is gonna be used to randomly generate business names in-game at some point but its also extremely fun to mess around with. &#10;Either you let the generator make random first or last names, or you put whatever you want.&#10;Keep pressing the button to get different results." display-tooltip-when-elided="true" style="font-size: 30px; -unity-font-definition: url(&apos;project://database/Assets/UI/Fonts/PTMono-Regular.ttf?fileID=12800000&amp;guid=cc1735a3c585aba4480bc03c8d93b029&amp;type=3#PTMono-Regular&apos;); flex-shrink: 1; flex-grow: 1; overflow: visible; white-space: normal; text-overflow: clip; align-self: center; -unity-text-align: upper-center; color: rgb(255, 255, 255);" />
                <ui:VisualElement style="flex-grow: 1; justify-content: space-around; align-items: stretch;">
                    <ui:Toggle label="Random First &amp; Last Names" name="RandomNames" value="true" class="titlescreenbuttons titlescreenboxes" style="font-size: 50px;" />
                    <ui:TextField picking-mode="Ignore" label="First Name" name="FirstName" class="titlescreenbuttons titlescreenboxes" style="font-size: 50px;" />
                    <ui:TextField picking-mode="Ignore" label="Last Name" name="LastName" class="titlescreenbuttons titlescreenboxes" style="font-size: 50px;" />
                    <ui:VisualElement style="flex-grow: 1; align-items: center; align-self: center; justify-content: space-between; flex-direction: row; flex-shrink: 0; max-height: 100px;">
