        <ui:VisualElement style="flex-grow: 1; background-image: url(&apos;project://database/Assets/UI/Logo/FullTallRender.png?fileID=2800000&amp;guid=4c4a09f4bb9559741a1b12c0ac4a034a&amp;type=3#FullTallRender&apos;); -unity-background-scale-mode: scale-to-fit; translate: 0 241px; scale: 1.25 1.25; transform-origin: 7% 90%;" />
    </ui:GroupBox>
